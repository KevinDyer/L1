{
  "name": "L1",
  "version": "0.0.1",
  "description": "Compiler as a service.",
  "author": "Art Compiler LLC",
  "license": "MIT",
  "main": "app.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/graffiticode/L1"
  },
  "scripts": {
    "start": "node app.js",
    "build": "babel-node tools/build",
    "smoke": "mocha --reporter spec --recursive --delay --smoke",
    "pretest": "npm run build",
    "test": "mocha -r babel-register --reporter spec --recursive --delay",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "watch": "npx nodemon -w src -w index.js -w package.json --exec make",
    "lambda:zip": "zip -r function.zip . -x=.git/*",
    "lambda:update": "aws lambda update-function-code --function-name L1 --zip-file fileb://function.zip",
    "deploy:lambda": "npm run build && npm run lambda:zip && npm run lambda:update",
    "deploy:gcloud": "gcloud functions deploy L1 --trigger-http --runtime=nodejs10 --entry-point=compiler"
  },
  "dependencies": {
    "aws-serverless-express": "^3.3.6",
    "body-parser": "^1.18.3",
    "domready": "^1.0.8",
    "express": "^4.0.x",
    "hashids": "1.1.1",
    "morgan": "^1.9.1",
    "request": "^2.88.0"
  },
  "engines": {
    "node": "latest"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-3"
    ]
  },
  "devDependencies": {
    "@pollyjs/adapter-node-http": "^2.4.0",
    "@pollyjs/core": "^2.4.0",
    "@pollyjs/persister-fs": "^2.1.0",
    "babel": "^6.0.x",
    "babel-cli": "^6.4.5",
    "babel-plugin-transform-es2015-modules-amd": "^6.8.0",
    "babel-preset-es2015": "^6.1.2",
    "babel-preset-react": "^6.1.2",
    "babel-preset-stage-3": "^6.24.1",
    "babelify": "^7.0.2",
    "browserify": "^12.0.1",
    "chai": "^4.2.0",
    "d3": "4.7.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "flow-bin": "^0.32.0",
    "jsdom": "^15.1.1",
    "json-diff": "^0.5.4",
    "mocha": "^6.1.3",
    "nodemon": "^1.19.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-test-renderer": "^16.8.6",
    "sinon": "^7.3.2",
    "snapshot-diff": "^0.5.1",
    "supertest": "^4.0.2",
    "uglify-js": "^2.7.3"
  }
}
